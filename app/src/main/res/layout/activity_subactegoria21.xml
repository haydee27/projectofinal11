<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/fondooooo"
    android:layout_margin="0dp"
    tools:context=".ui.categorias.subactegoria21">

    <Space
        android:layout_width="match_parent"
        android:layout_height="5dp"
        />
    <androidx.appcompat.widget.AppCompatCheckedTextView
        android:id="@+id/txtm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="90dp"
        android:layout_marginRight="0dp"
        android:fontFamily="@font/architects_daughter"
        android:foregroundTint="#101010"
        android:text="Categoria de bebidas"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="48dp" />


    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="100dp"
            android:layout_height="160dp"
            android:src="@drawable/resvaladerapanama"
            android:layout_marginRight="40dp"
            android:layout_marginLeft="60dp"/>

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="100dp"
            android:layout_height="160dp"
            android:src="@drawable/guarapopanama"
            android:layout_marginRight="40dp"/>

        <ImageView
            android:id="@+id/imageView53"
            android:layout_width="100dp"
            android:layout_height="160dp"
            android:src="@drawable/atoledeavenaypiloncillo"
            android:layout_marginRight="60dp"
            android:layout_marginLeft="0dp"/>

    </androidx.appcompat.widget.LinearLayoutCompat>


    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnpb4"
            android:layout_width="100dp"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:layout_height="wrap_content"
            android:onClick="bebidap4"
            android:text="RESBALADERA"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="15dp"
            android:textColor="@color/black"
            android:textSize="12dp" />


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnpb5"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:text="GUARAPO"
            android:onClick="bebidap5"
            android:layout_marginLeft="30dp"
            android:textColor="@color/black"
            android:textSize="15dp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btncb2"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:onClick="bebidac2"
            android:text="atole de avena y piloncillo"
            android:textSize="12dp"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="40dp"
            android:textColor="@color/black"
            />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <Space
        android:layout_width="match_parent"
        android:layout_height="5dp"
        />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="110dp"
            android:layout_height="160dp"
            android:src="@drawable/batidopina"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="0dp" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="100dp"
            android:layout_height="160dp"
            android:src="@drawable/batidodeuchuva"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="40dp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="100dp"
            android:layout_height="160dp"
            android:src="@drawable/cafefrioconmora"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="10dp"/>

    </androidx.appcompat.widget.LinearLayoutCompat>


    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btncb3"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:text="batido de piña"
            android:onClick="bebidac3"
            android:layout_marginLeft="0dp"
            android:textColor="@color/black"
            android:textSize="12dp"/>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btncb4"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:onClick="bebidac4"
            android:text="batido de uchuva"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="40dp"
            android:textColor="@color/black"
            android:textSize="12dp"/>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btncb5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:onClick="bebidac5"
            android:text="cafe frio con mora"
            android:textColor="@color/black"
            android:textSize="12dp"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="0dp"/>

    </androidx.appcompat.widget.LinearLayoutCompat>

    <Space
        android:layout_width="match_parent"
        android:layout_height="5dp"
        />
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="110dp"
            android:layout_height="160dp"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="40dp"
            android:src="@drawable/chichemepanamenopanama"/>

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="90dp"
            android:layout_height="100dp"
            android:src="@drawable/chichademaizpanama"
            android:layout_marginRight="40dp"
            android:layout_marginLeft="40dp"/>

        <ImageView
            android:id="@+id/imageView63"
            android:layout_width="110dp"
            android:layout_height="90dp"
            android:layout_marginRight="40dp"
            android:src="@drawable/chcichadesarilpanama"/>


    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnpb1"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:text="CHICHEME"
            android:onClick="bebidap1"
            android:textColor="@color/black"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="20dp"
            android:textSize="12dp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnpb2"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:textColor="@color/black"
            android:onClick="bebidap2"
            android:text="CHICHA DE MAIZ"
            android:layout_marginRight="40dp"
            android:layout_marginLeft="40dp"/>


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnpb3"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:text="CHICHA DE SARIL"
            android:onClick="bebidap3"
            android:textColor="@color/black"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="0dp"/>

    </androidx.appcompat.widget.LinearLayoutCompat>


    <Space
        android:layout_width="match_parent"
        android:layout_height="15dp"
        />
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/bm2"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#AC88CA"
            android:background="#020202"
            android:textColor="@color/black"
            android:text="Más bebidas"
            android:onClick="subactegoria21"/>

    </androidx.appcompat.widget.LinearLayoutCompat>


</androidx.appcompat.widget.LinearLayoutCompat>