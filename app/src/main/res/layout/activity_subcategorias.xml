<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/fondooooo"
    android:layout_margin="0dp"
    tools:context=".ui.categorias.subcategorias">

    <Space
        android:layout_width="match_parent"
        android:layout_height="5dp"
        />
    <androidx.appcompat.widget.AppCompatCheckedTextView
        android:id="@+id/txtm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="90dp"
        android:layout_marginRight="0dp"
        android:fontFamily="@font/architects_daughter"
        android:foregroundTint="#101010"
        android:text="Categoria de postres"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textColor="#000000"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="48dp" />


    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="100dp"
            android:layout_height="160dp"
            android:src="@drawable/pandeyemaguate"
            android:layout_marginRight="40dp"
            android:layout_marginLeft="60dp"/>

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="100dp"
            android:layout_height="160dp"
            android:src="@drawable/dulcedepapayamadurapanama"
            android:layout_marginRight="40dp"/>

        <ImageView
            android:id="@+id/imageView53"
            android:layout_width="100dp"
            android:layout_height="160dp"
            android:src="@drawable/hojaldrapanama"
            android:layout_marginRight="60dp"
            android:layout_marginLeft="0dp"/>

    </androidx.appcompat.widget.LinearLayoutCompat>


    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btngo5"
            android:layout_width="100dp"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:layout_height="wrap_content"
            android:text="pan de yema"
            android:onClick="postreg5"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="15dp"
            android:textColor="@color/black"
            android:textSize="15dp" />


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnpo1"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:onClick="postrep1"
            android:text="dulce de papaya"
            android:layout_marginLeft="30dp"
            android:textColor="@color/black"
            android:textSize="15dp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnpo2"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:text="hojaldra de panameña"
            android:onClick="postrep2"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="40dp"
            android:textColor="@color/black"
            android:textSize="15dp" />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <Space
        android:layout_width="match_parent"
        android:layout_height="5dp"
        />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="110dp"
            android:layout_height="160dp"
            android:src="@drawable/cocadapanama"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="0dp" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="100dp"
            android:layout_height="160dp"
            android:src="@drawable/mazamorrapanama"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="40dp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="100dp"
            android:layout_height="160dp"
            android:src="@drawable/pesadadenancepanama"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="10dp"/>

    </androidx.appcompat.widget.LinearLayoutCompat>


    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnpo3"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:onClick="postrep3"
            android:text="cocada panameña"
            android:layout_marginLeft="0dp"
            android:textColor="@color/black"
            android:textSize="10dp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnpo4"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:onClick="postrep4"
            android:text="mazamorra"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="40dp"
            android:textColor="@color/black"
            android:textSize="10dp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnpo5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:text="pesada de nance"
            android:onClick="postrep5"
            android:textColor="@color/black"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="0dp"/>

    </androidx.appcompat.widget.LinearLayoutCompat>

    <Space
        android:layout_width="match_parent"
        android:layout_height="5dp"
        />
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="110dp"
            android:layout_height="160dp"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="40dp"
            android:src="@drawable/temalitosdecambrayguate" />
        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="90dp"
            android:layout_height="100dp"
            android:src="@drawable/concervadecocoguate"
            android:layout_marginRight="40dp"
            android:layout_marginLeft="40dp"/>

        <ImageView
            android:id="@+id/imageView63"
            android:layout_width="110dp"
            android:layout_height="90dp"
            android:layout_marginRight="40dp"
            android:src="@drawable/jocotenemielguate"/>


    </androidx.appcompat.widget.LinearLayoutCompat>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnguate2"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:onClick="postreguate2"
            android:textColor="@color/black"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="20dp"
            android:text="tamalitos de cambray" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btngo3"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:text="Concerva de coco"
            android:textColor="@color/black"
            android:onClick="postreg3"
            android:layout_marginRight="40dp"
            android:layout_marginLeft="40dp"/>


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btngo4"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#E5DFE6"
            android:fontFamily="sans-serif-smallcaps"
            android:onClick="postreg4"
            android:textColor="@color/black"
            android:text="jocote en miel"
            android:layout_marginRight="20dp"
            android:layout_marginLeft="0dp"/>

    </androidx.appcompat.widget.LinearLayoutCompat>


    <Space
        android:layout_width="match_parent"
        android:layout_height="15dp"
        />
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/button"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#AC88CA"
            android:background="#020202"
            android:textColor="@color/black"
            android:text="Más postres" />

    </androidx.appcompat.widget.LinearLayoutCompat>
</androidx.appcompat.widget.LinearLayoutCompat>