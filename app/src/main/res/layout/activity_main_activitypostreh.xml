<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/fondooooo"
    android:layout_margin="0dp"
    tools:context=".ui.honduras.MainActivitypostreh"
    tools:ignore="MissingClass">

    <Space
        android:layout_width="match_parent"
        android:layout_height="55dp"
        />
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="160dp"
            android:layout_height="160dp"
            android:src="@drawable/pasteldegallina"
            android:layout_marginRight="60dp"
            android:layout_marginLeft="60dp"/>

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="160dp"
            android:layout_height="160dp"
            android:src="@drawable/alcitrones"
            android:layout_marginRight="60dp"/>
    </androidx.appcompat.widget.LinearLayoutCompat>


    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btnho1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PASTEL DE GALLINA"
            android:onClick="postreh1"
            android:layout_marginRight="60dp"
            android:layout_marginLeft="0dp"/>


        <Button
            android:id="@+id/btnho2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="postreh2"
            android:text="ALCITRONES" />

    </androidx.appcompat.widget.LinearLayoutCompat>
    <Space
        android:layout_width="match_parent"
        android:layout_height="25dp"
        />
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:src="@drawable/ciruelasenmiel"
            android:layout_marginRight="60dp"
            android:layout_marginLeft="0dp"/>

        <ImageView
            android:id="@+id/imageView63"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:src="@drawable/macheteadas"/>
    </androidx.appcompat.widget.LinearLayoutCompat>


    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btnho3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="CIRUELS EN MIEL"
            android:onClick="postreh3"
            android:layout_marginRight="60dp"
            android:layout_marginLeft="0dp"/>


        <Button
            android:id="@+id/btnho4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="postreh4"
            android:text="MACHETEADAS" />

    </androidx.appcompat.widget.LinearLayoutCompat>
    <Space
        android:layout_width="match_parent"
        android:layout_height="25dp"
        />
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:src="@drawable/mazapan" />

    </androidx.appcompat.widget.LinearLayoutCompat>
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal">
        <Button
            android:id="@+id/btnho5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="postreh5"
            android:text="MAZAPAN RELLENO" />
    </androidx.appcompat.widget.LinearLayoutCompat>

</androidx.appcompat.widget.LinearLayoutCompat>